<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: darkslategray" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.FieldCellController">
   <children>
      <ImageView fx:id="shipImage" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="false">
         <image>
            <Image url="stripes.png" />
         </image>
      </ImageView>
      <Circle fx:id="hitMarker" fill="DODGERBLUE" radius="15.0" stroke="BLACK" strokeType="INSIDE" />
   </children>
</StackPane>
